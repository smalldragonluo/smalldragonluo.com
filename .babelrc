{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ],
    "@babel/proposal-class-properties",
    "@babel/plugin-proposal-unicode-property-regex",
    "react-hot-loader/babel"
  ]
}